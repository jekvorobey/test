{
    "name": "ibt",
    "description": "A ibt project",
    "author": "Kolesnikov Maksim GREENSIGHT company",
    "private": true,
    "browserslist": {
        "production": [
            "> 0.25%",
            "ie 11",
            "ios 9.3",
            "not op_mini all",
            "not android <= 4.4",
            "not chrome <= 49"
        ],
        "development": [
            "last 2 versions"
        ],
        "ssr": [
            "node 11"
        ]
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu node -r dotenv/config --trace-deprecation server-local",
        "dev:remote": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu CONFIG=.env.remote.json node --trace-deprecation server-local",
        "dev:inspect": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu node --inspect server-local",
        "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node -r dotenv/config server-local",
        "start:watch": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu nodemon server-local",
        "start:remote": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu CONFIG=.env.remote.json node server-local",
        "rebuild": "rimraf ../public && yarn build:client && yarn build:server && yarn build:pages && yarn copy:files",
        "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
        "build:pages": "cross-env NODE_ENV=production webpack --config build/webpack.pages.config.js --progress --hide-modules",
        "copy:files": "cp -r ./src/assets/files/* ../public/assets/files/ && cp -r ./static/* ../public/",
        "analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config build/webpack.client.config.js --progress --hide-modules",
        "eslint": "eslint **/*.js **/*.vue --fix",
        "stylelint": "stylelint '**/*.css' --fix"
    },
    "engines": {
        "node": ">=8.0",
        "npm": ">=4.0"
    },
