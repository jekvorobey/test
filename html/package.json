{
    "name": "ibt",
    "description": "A ibt project",
    "author": "Kolesnikov Maksim GREENSIGHT company",
    "private": true,
    "browserslist": {
        "production": [
            "> 0.25%",
            "ie 11",
            "ios 9.3",
            "not op_mini all",
            "not android <= 4.4",
            "not chrome <= 49"
        ],
        "development": [
            "last 2 versions"
        ],
        "ssr": [
            "node 11"
        ]
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development node server-local",
        "start": "cross-env NODE_ENV=production node server-local",
        "start:host": "cross-env NODE_ENV=production CONFIG=.env node server-local",
        "rebuild": "rimraf ../public/assets && yarn build:client && yarn build:server",
        "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
        "analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config build/webpack.client.config.js --progress --hide-modules",
        "eslint": "eslint **/*.js --fix",
        "stylelint": "stylelint '**/*.css' --fix"
    },
    "engines": {
        "node": ">=7.0",
        "npm": ">=4.0"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "axios-extensions": "^3.0.6",
        "chalk": "^2.4.2",
        "compression": "^1.7.4",
        "core-js": "^3.3.4",
        "cross-env": "^6.0.3",
        "custom-event-polyfill": "^1.0.7",
        "express": "^4.17.1",
        "filepond": "^4.7.2",
        "filepond-plugin-file-validate-type": "^1.2.4",
        "filepond-plugin-image-preview": "^4.5.0",
        "flatpickr": "^4.6.3",
        "focus-visible": "^5.0.2",
        "intersection-observer": "^0.7.0",
        "lazysizes": "^5.1.2",
        "lodash": "^4.17.15",
        "lru-cache": "^5.1.1",
        "memory-fs": "^0.5.0",
        "nouislider": "^14.0.3",
        "nuxt-dropzone": "^1.0.4",
        "regenerator-runtime": "^0.13.3",
        "route-cache": "0.4.4",
        "serve-favicon": "^2.5.0",
        "v-tooltip": "^2.0.2",
